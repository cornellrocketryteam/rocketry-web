/ const whoWeAreContainer = useRef();\r\n  const whoWeAreRef = useRef();\r\n\r\n  useEffect(() => {\r\n    ScrollLottie({\r\n      target: animRef.current,\r\n      path: '../static/lotties/highResAnim.json',\r\n      duration: 3800 * 2,\r\n      acceleration: 0.3,\r\n    });\r\n  }, [animRef]);\r\n\r\n  useEffect(() => {\r\n    let timeline = gsap.timeline({\r\n      scrollTrigger: {\r\n        trigger: statsRef.current,\r\n        pin: true,\r\n        scrub: true,\r\n        start: `top ${\r\n          50 - (statsRef.current.clientHeight / window.innerHeight) * 50\r\n        }%`, // makes the content appear in the center of the screen\r\n        end: '+=800',\r\n        // markers: true, //debug markers\r\n      },\r\n    });\r\n\r\n    let timeline2 = gsap.timeline({\r\n      scrollTrigger: {\r\n        trigger: whoWeAreRef.current,\r\n        pin: true,\r\n        scrub: true,\r\n        start: `top ${\r\n          50 - (whoWeAreRef.current.clientHeight / window.innerHeight) * 50\r\n        }%`, // makes the content appear in the center of the screen\r\n        end: '+=1200',\r\n        // markers: true, //debug markers\r\n      },\r\n    });\r\n\r\n    timeline\r\n      .addLabel('start')\r\n      .fromTo(\r\n        statsRef.current,\r\n        { opacity: 0, yPercent: 5 },\r\n        { duration: 2, opacity: 1, yPercent: 0 },\r\n        '-=1'\r\n      )\r\n      .addLabel('middle')\r\n      .to(\r\n        statsRef.current,\r\n        {\r\n          duration: 2,\r\n          opacity: 0,\r\n          yPercent: -5,\r\n          display: 'none',\r\n        },\r\n        '+=1'\r\n      )\r\n      .addLabel('end');\r\n\r\n    timeline2\r\n      .addLabel('start')\r\n      .fromTo(\r\n        whoWeAreRef.current,\r\n        { opacity: 0, yPercent: 5 },\r\n        { duration: 2, opacity: 1, yPercent: 0 },\r\n        '-=1'\r\n      )\r\n      .addLabel('middle')\r\n      .to(\r\n        whoWeAreRef.current,\r\n        {\r\n          duration: 2,\r\n          opacity: 0,\r\n          yPercent: -5,\r\n          display: 'none',\r\n        },\r\n        '+=1'\r\n      )\r\n      .addLabel('end');\r\n  }, [statsRef, whoWeAreRef]);\r\n\r\n  return (\r\n    <div className={classes.root}>\r\n      <Header />\r\n      <div className={classes.animationContainer}>\r\n        <div ref={animRef} className={classes.animation}></div>\r\n      </div>\r\n      <div className={classes.content}>\r\n        <div className={classes.h100}>\r\n          <Typography variant='h1' className={(classes.title, classes.center)}>\r\n            Cornell Rocketry Team\r\n          </Typography>\r\n        </div>\r\n\r\n        <Container className={classes.relative}>\r\n          <div ref={statsRef} className={classes.statistics}>\r\n            <Box mb={12}>\r\n              <Typography variant='h2'>At a Glance</Typography>\r\n            </Box>\r\n            <Grid container direction='row' justify='space-between' spacing={3}>\r\n              <StatsNumber number={40} label={'Team Members'} />\r\n              <StatsNumber number={6} label={'Subteams'} />\r\n              <StatsNumber number={6} label={'Majors'} />\r\n              <StatsNumber number={5} label={'Rockets Launched'} />\r\n            </Grid>\r\n          </div>\r\n        </Container>\r\n\r\n        <Container className={classes.relative} maxWidth='md'>\r\n          <div ref={whoWeAreRef} className={classes.statistics}>\r\n            <Box mb={6}>\r\n              <Typography variant='h2'>Who We Are</Typography>\r\n            </Box>\r\n            <Typography variant='h5'>\r\n              Cornell Rocketry is an engineering project team dedicated to\r\n              designing, assembling, and launching high-powered rockets to\r\n              compete in the annual Spaceport America Cup.\r\n            </Typography>\r\n          </div>\r\n        </Container>\r\n\r\n        <Container className={classes.relative} maxWidth>\r\n          <Typography variant='h5'>\r\n            The vertical dri